{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-starter",
  "author": "Rafael Menezes",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.tsx'",
    "scss-lint": "stylelint 'src/**/*.scss'",
    "scss-types": "typed-scss-modules  --implementation sass -e named 'src/**/*.scss'",
    "scss-types-watch": "yarn scss-types --watch",
    "prettifyTS": "prettier 'src/**/*.tsx' --write",
    "prettifyCSS": "prettier 'src/**/*.scss' --write",
    "test": "jest --config test/jest.config.js",
    "clean": "gatsby clean",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.scss": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "gatsby": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-sass": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-jest": "^29.3.1",
    "babel-preset-gatsby": "^3.2.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.0",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "typed-scss-modules": "^7.0.1",
    "typescript": "^4.8.4"
  }
}
